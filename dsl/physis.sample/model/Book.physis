dataaccess {
	contentprovider BookContentProvider
		url: http://peterfriese.local/~peterfriese/books/books.json
		mapper: BookMapper	
}

mappings {
	mapper BookMapper for entity Book {
		title = title
		author = author
		publisher = publisher
	}
}

datamodel default {

	entity Book {
		title: string
		author: string
		publisher: string
	}
}